{
  "manifest_version": 3,
  "name": "Tab Duplicator",
  "version": "1.0",
  "description": "Duplicate the current tab using a keyboard shortcut.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "duplicate-tab": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Duplicate the current tab"
    }
  }
}
